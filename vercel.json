{
  "version": 2,
  "public": true,
  "env": {},
  "routes": [
    {
      "src": "/api/auth/(.*)",
      "dest": "/api/auth/$1"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/login",
      "dest": "/login.html"
    },
    {
      "src": "/register",
      "dest": "/register.html"
    },
    {
      "src": "/Leave",
      "dest": "/leave.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
